<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="dms.data.SiteCity" table="site_city">
<id column="ID" name="id" type="java.lang.Long">
<generator class="identity"/>
</id>
<property column="SITE_ID" length="11" name="siteId" type="java.lang.Integer"/>
<property column="SORT_ORDER" length="11" name="sortOrder" type="java.lang.Integer"/>
<property column="CITY" length="11" name="city" type="java.lang.Integer"/>
<property column="SERVICE" length="11" name="service" type="java.lang.Integer"/>
<property column="URL" length="254" name="url" type="java.lang.String"/>
<property column="CREATE_TIME" length="19" name="createTime" type="java.util.Date"/>
<property column="EDIT_TIME" length="19" name="editTime" type="java.util.Date"/>
</class>
</hibernate-mapping>
