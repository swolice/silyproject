<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="dms.data.Vote" table="vote">
<id column="ID" name="id" type="java.lang.Long">
<generator class="identity"/>
</id>
<property column="USER_ID" length="11" name="userId" type="java.lang.Integer"/>
<property column="INFO_ID" length="11" name="infoId" type="java.lang.Integer"/>
<property column="CONTENT" length="65535" name="content" type="java.lang.String"/>
<property column="VOTE_TYPE" length="11" name="voteType" type="java.lang.Integer"/>
<property column="CREATE_TIME" length="19" name="createTime" type="java.util.Date"/>
<property column="EDIT_TIME" length="19" name="editTime" type="java.util.Date"/>
</class>
</hibernate-mapping>
