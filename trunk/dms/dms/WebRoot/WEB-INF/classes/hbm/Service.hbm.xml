<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="dms.data.Service" table="service">
<id column="ID" name="id" type="java.lang.Long">
<generator class="identity"/>
</id>
<property column="NAME" length="255" name="name" type="java.lang.String"/>
<property column="PIC" length="255" name="pic" type="java.lang.String"/>
<property column="REMARK" length="255" name="remark" type="java.lang.String"/>
</class>
</hibernate-mapping>
